{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier", "react-app", "react-app/jest"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      { "arrowParens": "avoid", "endOfLine": "crlf" }
    ],
    "no-console": 0
  }
}
